<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp1.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**\*.cs" />
    <EmbeddedResource Include="**\*.resx" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="bin\**" />
    <EmbeddedResource Remove="bin\**" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Discord.Net" Version="1.0.0-beta2-00522" />
    <PackageReference Include="Discord.Net.Commands" Version="1.0.0-beta2-00522" />
    <PackageReference Include="Discord.Net.Core" Version="1.0.0-beta2-00522" />
    <PackageReference Include="Discord.Net.Rest" Version="1.0.0-beta2-00522" />
    <PackageReference Include="Discord.Net.Rpc" Version="1.0.0-beta2-00522" />
    <PackageReference Include="Discord.Net.WebSocket" Version="1.0.0-beta2-00522" />
    <PackageReference Include="Microsoft.NETCore.App" Version="1.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />
  </ItemGroup>
</Project>